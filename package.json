{
  "name": "code2doc",
  "version": "0.0.1",
  "description": "auto generate doc from your code",
  "main": "src/index.ts",
  "types": "src/types",
  "scripts": {
    "test": "jest",
    "lint": "eslint --ext .ts --fix src/",
    "commit": "git cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SeNsuos/code2doc.git"
  },
  "keywords": [
    "code2doc"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SeNsuos/code2doc/issues"
  },
  "homepage": "https://github.com/SeNsuos/code2doc#readme",
  "dependencies": {
    "@babel/parser": "^7.12.5",
    "@types/babel-core": "^6.25.6",
    "@types/glob": "^7.1.3",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "babel-core": "^6.26.3",
    "glob": "^7.1.6"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^4.7.0",
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint-config-google": "^0.14.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "typescript": "^4.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
